! Stack implementation using the system class SIMSET;
Simset class Stacks;
begin
    Head class Stack;
    begin
        procedure Push(x);
            ref(Link) x;
            x.Follow(this Head);

        ref(Link) procedure Pop;
        begin
           Pop :- First;
           First.Out;
        end Pop;

        integer procedure Size;
            Size := Cardinal;
    end Stack;
end Stacks;
